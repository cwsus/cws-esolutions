<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://geronimo.apache.org/xml/ns/j2ee/web-2.0.1">
    <dep:environment xmlns:dep="http://geronimo.apache.org/xml/ns/deployment-1.2">
        <dep:moduleId>
            <dep:groupId>@groupId@</dep:groupId>
            <dep:artifactId>@artifactId@</dep:artifactId>
            <dep:version>@version@</dep:version>
            <dep:type>war</dep:type>
        </dep:moduleId>

        <dep:dependencies>
            <dep:dependency>
                <dep:groupId>org.apache.geronimo.configs</dep:groupId>
                <dep:artifactId>sharedlib</dep:artifactId>
                <dep:version>3.0.1</dep:version>
                <dep:type>car</dep:type>
            </dep:dependency>
            <dependency>
                <groupId>org.apache.geronimo.configs</groupId>
                <artifactId>javamail</artifactId>
                <version>3.0.1</version>
                <type>car</type>
            </dependency>
            <dep:dependency>
                <dep:groupId>console.jms</dep:groupId>
                <dep:artifactId>cws</dep:artifactId>
                <dep:version>1.0</dep:version>
                <dep:type>car</dep:type>
            </dep:dependency>
            <dep:dependency>
                <dep:groupId>console.dbpool</dep:groupId>
                <dep:artifactId>jdbc_esolutionssvc</dep:artifactId>
                <dep:version>1.0</dep:version>
                <dep:type>car</dep:type>
            </dep:dependency>
            <dep:dependency>
                <dep:groupId>console.dbpool</dep:groupId>
                <dep:artifactId>jdbc_cwssec</dep:artifactId>
                <dep:version>1.0</dep:version>
                <dep:type>car</dep:type>
            </dep:dependency>
        </dep:dependencies>

        <dep:hidden-classes>
			<dep:filter>org.apache.log4j</dep:filter>
            <dep:filter>log4j</dep:filter>
        </dep:hidden-classes>

        <dep:inverse-classloading />
    </dep:environment>

    <context-root>@contextRoot@</context-root>

    <nam:service-ref xmlns:nam="http://geronimo.apache.org/xml/ns/naming-1.2">
        <nam:service-ref-name>services/CoreRequestProcessorService</nam:service-ref-name>
    </nam:service-ref>

    <nam:resource-ref xmlns:nam="http://geronimo.apache.org/xml/ns/naming-1.2">
        <nam:ref-name>jdbc/esolutionssvc</nam:ref-name>
        <nam:pattern>
            <nam:groupId>console.dbpool</nam:groupId>
            <nam:artifactId>jdbc_esolutionssvc</nam:artifactId>
            <nam:version>1.0</nam:version>
            <nam:name>jdbc/esolutionssvc</nam:name>
        </nam:pattern>
    </nam:resource-ref>
    <nam:resource-ref xmlns:nam="http://geronimo.apache.org/xml/ns/naming-1.2">
        <nam:ref-name>jdbc/cwssec</nam:ref-name>
        <nam:pattern>
            <nam:groupId>console.dbpool</nam:groupId>
            <nam:artifactId>jdbc_cwssec</nam:artifactId>
            <nam:version>1.0</nam:version>
            <nam:name>jdbc/cwssec</nam:name>
        </nam:pattern>
    </nam:resource-ref>
    <nam:resource-ref xmlns:nam="http://geronimo.apache.org/xml/ns/naming-1.2">
        <nam:ref-name>mail/MailSession</nam:ref-name>
        <nam:pattern>
            <nam:groupId>org.apache.geronimo.configs</nam:groupId>
            <nam:artifactId>javamail</nam:artifactId>
            <nam:version>3.0.1</nam:version>
            <nam:name>mail/MailSession</nam:name>
        </nam:pattern>
    </nam:resource-ref>
</web-app>
